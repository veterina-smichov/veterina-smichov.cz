{
    "name": "{{ site.siteName }}",
    "short_name": "{{ site.siteName }}",
    "icons": [
        {
            "src": "/assets/images/favicon/web-app-manifest-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "/assets/images/favicon/web-app-manifest-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "theme_color": "#2e7d7d",
    "background_color": "#ffffff",
    "display": "standalone"
}
